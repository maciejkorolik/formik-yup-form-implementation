{
  "extends": ["react-app", "airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "env": {
    "jest": true
  },
  "globals": {
    "document": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js"]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0
  }
}
